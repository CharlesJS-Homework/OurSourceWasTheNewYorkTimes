{
    "extends": "airbnb-base",
    "rules": {
        "prefer-destructuring": "off",
        "no-use-before-define": ["error", { "functions": false, "classes": true }],
        "no-plusplus": "off",
        "no-underscore-dangle": "off",
        "radix": "off"
    },
    "env": {
        "es6": true
    }
}